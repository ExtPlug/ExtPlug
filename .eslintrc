{
  "extends": "airbnb-base",
  "env": {
    "browser": true
  },
  "globals": {
    "API": false
  },
  "rules": {
    "no-param-reassign": ["error", { "props": false }],
    "no-underscore-dangle": ["error", { "allow": ["_super"] }],
    "import/no-extraneous-dependencies": "off",
    "import/no-unresolved": ["error", {
      "ignore": ["^plug/", "^lang/Lang$"]
    }],
    "import/extensions": "off"
  }
}
